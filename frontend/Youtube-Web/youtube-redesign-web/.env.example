# Which auth/backend strategy to use: custom | firebase | supabase
VITE_BACKEND_MODE=custom

# Used by Vite proxy and socket client for custom backend mode
VITE_BACKEND_URL=http://localhost:5001

# Used by @react-oauth/google provider (only needed if your flow uses it)
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Firebase mode (required only when VITE_BACKEND_MODE=firebase)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# Supabase mode (required only when VITE_BACKEND_MODE=supabase)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
